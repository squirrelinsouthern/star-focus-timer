button::after{
  border: none;
}
.button-hover {
  opacity: 0.8 !important;
}

.default-button{
  display:flex;
  align-items: center;
  justify-content: center;
  width:360rpx;
  height: 96rpx;
  border-radius: 54rpx;
  color:#34243E;
  background-color: white;
  font-size:32rpx;
}
.default-button.mini{
  width:340rpx;
}
.default-button.transparent{
  background-color: transparent;
  border:1rpx solid white;
  color:white;
}